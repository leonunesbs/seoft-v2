{
  "type": "module",
  "name": "seoft",
  "description": "Plataforma auxiliar do Setor de Oftalmologia do HGF, desenvolvida com Next.js e TypeScript para otimizar o dia a dia dos residentes e colaboradores.",
  "version": "1.0.0",
  "private": true,
  "author": "Leonardo Nunes <leonunesb@gmail.com> (@leonunesbs)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript",
    "oftalmologia",
    "HGF",
    "SEOFT",
    "residência médica",
    "tecnologia"
  ],
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev --turbo",
    "format": "prettier . --write",
    "format:ci": "prettier --list-different .",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "prisma generate && husky",
    "lint": "eslint \"src/**/*.+(ts|js|tsx)\"",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint \"src/**/*.+(ts|js|tsx)\" --fix",
      "prettier . --write"
    ]
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.2",
    "@aws-sdk/client-s3": "^3.701.0",
    "@aws-sdk/s3-request-presigner": "^3.701.0",
    "@hookform/resolvers": "^3.9.1",
    "@prisma/client": "^6.0.0",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@t3-oss/env-nextjs": "^0.11.0",
    "@tanstack/react-query": "^5.50.0",
    "@trpc/client": "^11.0.0-rc.446",
    "@trpc/react-query": "^11.0.0-rc.446",
    "@trpc/server": "^11.0.0-rc.446",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "geist": "^1.3.0",
    "lucide-react": "^0.462.0",
    "next": "^15.0.1",
    "next-auth": "5.0.0-beta.25",
    "next-themes": "^0.4.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-icons": "^5.3.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/eslintrc": "^3.2.0",
    "@types/eslint": "^9.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "8.18.1",
    "@typescript-eslint/parser": "8.18.1",
    "@vitest/coverage-v8": "^2.1.6",
    "eslint": "9.15.0",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^6.0.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.5.3",
    "vitest": "^2.1.6",
    "vitest-mock-extended": "^2.0.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.38.1"
  }
}
